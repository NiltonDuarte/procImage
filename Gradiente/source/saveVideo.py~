i = 0

cap = cv2.VideoCapture('Face3.mp4')
ret = True

while(cap.isOpened() and ret):
	ret, frame = cap.read()


	if ret:


		
		cv2.circle(frame, rightPos[i], 3,  255, 2, 8, 0 )
		cv2.circle(frame, leftPos[i], 3,  255, 2, 8, 0 )


		video.write(frame)
		cv2.imshow('frame',frame)
		if cv2.waitKey(1) & 0xFF == ord('q'):
			break
		i +=1





cap.release()
video.release()
cv2.destroyAllWindows()

